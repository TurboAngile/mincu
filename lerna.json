{
  "packages": [
    "packages/*",
    "example/*"
  ],
  "command": {
    "bootstrap": {
      "noCi": true,
      "npmClientArgs": ["--no-package-lock"]
    },
    "publish": {
      "message": ":bookmark: release: publish %s",
      "registry": "https://registry.npmjs.org/"
    }
  },
  "useWorkspaces": true,
  "npmClient": "yarn",
  "version": "1.1.0"
}
