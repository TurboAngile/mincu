{
  "verbose": false,
  "debug": false,
  "exec": "ts-node  ./index.ts",
  "ignore": [
    "mochawesome-report",
    "node_modules",
    "./test",
    "**/*.d.ts",
    "*.test.ts",
    "*.spec.ts",
    "fixtures/*",
    "test/**/*",
    "docs/*"
  ],
  "events": {
    "restart": ""
  },
  "watch": [
    "./packages",
    "./lib",
    "./index.ts"
  ],
  "ext": "ts tsx",
  "inspect": true
}